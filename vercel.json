{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "functions": {
        "backend/api/*.js": {
            "runtime": "nodejs18.x"
        }
    }
}